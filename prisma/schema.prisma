generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model News {
  id        Int      @id @default(autoincrement())
  title     String
  body      String
  imageUrl  String
  author    String
  date      DateTime
}